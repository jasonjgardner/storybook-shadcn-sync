{
  "name": "storybook-shadcn-demo",
  "version": "1.0.0",
  "description": "Demo project showing Storybook to shadcn/ui sync",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
