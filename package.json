{
  "name": "storybook-shadcn-sync",
  "version": "1.0.0",
  "description": "A comprehensive tool for syncing Storybook stories with shadcn/ui components and exporting to registry JSON format",
  "main": "dist/index.js",
  "bin": {
    "storybook-sync": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "storybook",
    "shadcn",
    "ui",
    "components",
    "registry",
    "sync",
    "cli",
    "typescript"
  ],
  "author": "Manus AI",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "zod": "^3.22.4",
    "typescript": "^5.3.3",
    "glob": "^10.3.10",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "prettier": "^3.1.1"
  }
}

